sudo pacman -S git git-lfs
git-lfs install
mkdir APPS
cd APPS
git clone https://aur.archlinux.org/yay.git
cd yay
makepkg -si
cd ~
rm APPS/ -Rf
yay -S timeshift firefox

yay -S tlp
sudo systemctl enable tlp
sudo systemctl start tlp

yay -S bumblebee nvidia mesa xf86-video-intel mesa-demos --needed
sudo gpasswd -a djinnalexio bumblebee
